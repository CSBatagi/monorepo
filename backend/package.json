{
  "name": "csbatagi-backend",
  "version": "1.0.0",
  "description": "A middleware for postgresql",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest --runInBand --verbose",
    "test:integration": "INTEGRATION_BACKEND_URL=http://localhost:3001 jest --runInBand --verbose test/integration"
  },
  "dependencies": {
    "@google-cloud/compute": "^4.1.0",
    "ajv": "^8.17.1",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "firebase-admin": "^13.6.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.13.1",
    "rcon": "^1.1.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "verbose": false,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/integration/"
    ],
    "collectCoverage": false
  }
}
