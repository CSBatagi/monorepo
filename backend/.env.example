# Backend Environment Variables
# Copy this file to .env and fill in your actual values
# Or run: .\setup-local-env.ps1 which will use your existing .backend_secrets file

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_DATABASE=csdm

# Authentication
AUTH_TOKEN=your_auth_token_here

# CS2 Server RCON
RCON_PASSWORD=your_rcon_password
RCON_HOST=localhost
RCON_PORT=27015

# GCP Configuration (optional for local development)
VM_NAME=cs2-server
GCP_ZONE=europe-west3-c
GOOGLE_APPLICATION_CREDENTIALS=../credentials.json

# Season Configuration (optional - defaults to 2025-06-09)
# SEZON_BASLANGIC=2025-06-09
# SEASON_START_FILE=/path/to/season_start.json

# Server Configuration
PORT=3000
NODE_ENV=development
